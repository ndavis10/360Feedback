@using _360Feedback.Models;
@model IEnumerable<Question>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@foreach(Question q in Model)
{
    <h3>@q.title</h3>
    <ul>
        @foreach (Category c in q.categories)
        {
            <li>
                @c.name
                <ul>
                    @foreach (Description s in c.Descriptions)
                    {
                        <li>@s.Value</li>
                    }
                </ul>
            </li>
        }
    </ul>
}